<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
 		http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="client" class="com.spring.Client">
        <constructor-arg type = "java.lang.String" index = "0" value = "1"/>
        <constructor-arg type = "java.lang.String" index = "1" value = "Oleksandr Roienko"/>
    </bean>

    <bean id="eventLogger" class="com.spring.ConsoleEventLogger"/>

    <bean id="app" class="com.spring.App">
        <constructor-arg ref = "client"/>
        <constructor-arg ref = "eventLogger"/>
    </bean>

</beans>
